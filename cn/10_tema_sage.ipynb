   "source": [
    "def newton_cotes(n):\n",
    "    res = 0\n",
    "    f = function(\"f\")\n",
    "    a, b = var(\"a, b\")\n",
    "    res += (b-a) / (2*n)\n",
    "    res += f(a)\n",
    "    res += f(b)\n",
    "    for k in range(1, n):\n",
    "        x_k = var(f\"x_{k}\")\n",
    "        res += f(x_k)\n",
    "    \n",
    "    return res\n",
    "\n",
    "pretty_print(newton_cotes(10))"
   ]
