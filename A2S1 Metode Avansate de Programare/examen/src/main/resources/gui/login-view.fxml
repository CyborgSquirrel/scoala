<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="gui.LogInController"
      fx:id="vboxMain">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>
    <HBox>
        <Label prefWidth="80" text="username"/>
        <ComboBox fx:id="comboBoxUsername" onAction="#onComboBoxUsernameAction"/>
    </HBox>

    <Hyperlink text="register" onAction="#onHyperlinkRegisterAction"/>
</VBox>
