<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="gui.AcceptFriendRequestController"
      alignment="CENTER" spacing="20.0">
    <TableView fx:id="tableView">
        <columns>
            <TableColumn fx:id="tableView_name" text="Nume"/>
            <TableColumn fx:id="tableView_createdTime" text="Trimis"/>
        </columns>
    </TableView>
    <VBox alignment="CENTER" spacing="20.0">
        <TextField fx:id="textFieldName" onKeyTyped="#onFriendNameTyped" />
        <Button text="accept" onAction="#onAcceptFriendButtonClicked"/>
    </VBox>
</HBox>
