<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="gui.StaffController">
    <Label text="Placed orders"/>
    <TableView fx:id="placedOrders">
        <columns>
            <TableColumn fx:id="placedOrdersId" text="table id" sortable="false" prefWidth="50"/>
            <TableColumn fx:id="placedOrdersDate" text="date" sortable="false" prefWidth="200"/>
            <TableColumn fx:id="placedOrdersItems" text="items" sortable="false" prefWidth="400"/>
        </columns>
    </TableView>
</VBox>
